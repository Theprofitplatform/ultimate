{
  "name": "hive-2.0-enterprise",
  "version": "2.0.0",
  "description": "Enterprise-grade multi-agent orchestration system with AI capabilities",
  "main": "orchestrator.js",
  "scripts": {
    "start": "node orchestrator.js",
    "dev": "nodemon orchestrator.js",
    "agent:backend": "node agents/backend.agent.js",
    "agent:frontend": "node agents/frontend.agent.js",
    "agent:seo": "node agents/seo-ai-optimizer.agent.js",
    "agent:database": "node agents/database.agent.js",
    "agent:devops": "node agents/devops.agent.js",
    "agent:testing": "node agents/testing.agent.js",
    "test": "jest --coverage --detectOpenHandles",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:integration": "jest --testPathPattern=integration --coverage",
    "test:e2e": "jest --testPathPattern=e2e --runInBand",
    "lint": "eslint . --ext .js,.ts",
    "format": "prettier --write '**/*.{js,ts,json,md}'",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker-compose build",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "monitor": "node scripts/monitor.js",
    "benchmark": "node scripts/benchmark.js"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "natural": "^6.10.0",
    "node-cron": "^3.0.3",
    "opossum": "^8.1.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "redis": "^4.6.12",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.19.0",
    "@opentelemetry/exporter-prometheus": "^0.46.0",
    "@opentelemetry/instrumentation-express": "^0.35.0",
    "@opentelemetry/instrumentation-http": "^0.46.0",
    "@opentelemetry/instrumentation-redis": "^0.36.0",
    "@opentelemetry/sdk-metrics": "^1.19.0",
    "@opentelemetry/sdk-node": "^0.46.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-node": "^1.19.0",
    "bull": "^4.12.0",
    "bullmq": "^5.1.1",
    "elastic-apm-node": "^4.3.0",
    "puppeteer": "^21.7.0",
    "playwright": "^1.40.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
